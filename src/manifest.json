{
    "manifest_version": 2,
    "name": "Twitter Icon Come Back",
    "version": "1.1.0",
    "description": "Twitterの丸くなったアイコンを元に戻します",
    "content_scripts": [{
        "matches": [
            "https://twitter.com/*",
            "https://tweetdeck.twitter.com/"
        ],
        "css": ["style.css"],
        "run_at": "document_start"
    }]
}
